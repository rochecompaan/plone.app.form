<ul class="uberselectionWidgetResults">
  <tal:results repeat="term options/results">
    <li tal:attributes="usw-parent term/parent_token;  
                        usw-browse term/browse_token;">
      <input type="hidden"
             tal:attributes="name string:${options/name};
                             value term/token" />

      <span class="title"
            tal:attributes="title term/description | nothing"
            tal:content="term/title">title</span>

      <span class="discreet description" 
            tal:condition="term/description | nothing"
            tal:content="term/description" />

      <a href=""
         class="uswBrowse"
         tal:condition="python:term.browse_token is not None"
         tal:attributes="href term/browse_token">&gt;</a>

    </li>
  </tal:results>
</ul>
